  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^22.10.5",
    "dotenv-cli": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
